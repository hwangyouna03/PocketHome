<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Community">

    <ImageView
        android:id="@+id/Logo2"
        android:layout_width="134dp"
        android:layout_height="53dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:src="@drawable/logo2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/profile"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:src="@drawable/profile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <view
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="76dp"
        android:layout_marginBottom="746dp"
        android:background="#B7B7B7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.033" />

    <ImageButton
        android:id="@+id/Question_button"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="826dp"
        android:background="@drawable/buttonround"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/knowledge_button"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:layout_marginStart="112dp"
        android:layout_marginBottom="826dp"
        android:background="@drawable/buttonround"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/review_button"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:layout_marginStart="208dp"
        android:layout_marginBottom="826dp"
        android:background="@drawable/buttonround"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/post_button"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:layout_marginStart="484dp"
        android:layout_marginBottom="826dp"
        android:background="@drawable/buttonround"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/title"
        android:layout_width="569dp"
        android:layout_height="52dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="160dp"
        android:background="@drawable/id"
        android:gravity="left|center"
        android:hint="제목"
        android:inputType="text"
        android:textColorHint="#000000"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/image"
        android:layout_width="569dp"
        android:layout_height="52dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="220dp"
        android:background="@drawable/id"
        android:gravity="left|center"
        android:hint="첨부파일"
        android:inputType="text"
        android:textColorHint="#000000"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/content"
        android:layout_width="569dp"
        android:layout_height="550dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="280dp"
        android:background="@drawable/id"
        android:gravity="left|top"
        android:hint="내용"
        android:inputType="text"
        android:textColorHint="#000000"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/Question_text"
        android:layout_width="50dp"
        android:layout_height="30dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="99dp"
        android:gravity="center"
        android:text="질문"
        android:textColor="#1459D4"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/knowledge_text"
        android:layout_width="50dp"
        android:layout_height="30dp"
        android:layout_marginStart="127dp"
        android:layout_marginTop="99dp"
        android:gravity="center"
        android:text="지식"
        android:textColor="#1459D4"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/review_text"
        android:layout_width="50dp"
        android:layout_height="30dp"
        android:layout_marginStart="224dp"
        android:layout_marginTop="99dp"
        android:gravity="center"
        android:text="후기"
        android:textColor="#1459D4"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/post_text"
        android:layout_width="90dp"
        android:layout_height="30dp"
        android:layout_marginStart="488dp"
        android:layout_marginTop="99dp"
        android:gravity="center"
        android:text="게시하기"
        android:textColor="#1459D4"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <view
        android:id="@+id/view2"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="852dp"
        android:background="#B7B7B7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/news_d"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="875dp"
        android:background="@drawable/news_d"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/information_d"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="200dp"
        android:layout_marginTop="875dp"
        android:background="@drawable/information_d"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/map_d"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="350dp"
        android:layout_marginTop="875dp"
        android:background="@drawable/map_d"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/community_d"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="500dp"
        android:layout_marginTop="875dp"
        android:background="@drawable/community_d"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>